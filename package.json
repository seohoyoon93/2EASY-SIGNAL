{
  "name": "2easy-signal",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "cheerio": "^1.0.0-rc.2",
    "express": "^4.16.4",
    "firebase": "^5.8.6",
    "lodash": "^4.17.11",
    "node-sass": "^4.11.0",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "react-redux": "^6.0.1",
    "react-redux-firebase": "^2.2.6",
    "react-scripts": "2.1.8",
    "redux": "^4.0.1",
    "redux-firestore": "^0.7.2",
    "redux-thunk": "^2.3.0",
    "request-promise": "^4.2.4",
    "semantic-ui-react": "^0.85.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "prebuild": "rm -r dist && mkdir dist",
    "build": "sh -ac '. .env.${REACT_APP_ENV}; react-scripts build'",
    "build:dev": "REACT_APP_ENV=dev npm run build && cp -a build/. dist/ && mv dist/robots.dev.txt dist/robots.txt",
    "build:staging": "REACT_APP_ENV=staging npm run build && cp -a build/. dist/ && mv dist/robots.dev.txt dist/robots.txt",
    "build:prod": "REACT_APP_ENV=prod npm run build && rm dist/robots.txt",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
